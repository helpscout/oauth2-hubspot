{
    "name": "helpscout/oauth2-hubspot",
    "description": "HubSpot OAuth 2 Client Provider for The PHP League OAuth2-Client package",
    "authors": [
        {
            "name": "Platform Group",
            "email": "developer@helpscout.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "hubspot"
    ],
    "require": {
        "league/oauth2-client": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "HelpScout\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HelpScout\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    }
}
